{

    "manifest_version": 2,
    "name": "Depretensioner",
    "version": "1.0",

    "description": "Replaces occurances of the word 'whilst' with 'while', and 'amongst' with 'among'.",

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["de-pretensioner.js"]
        }
    ],

    "applications": {
        "gecko": {
            "id": "depretensioner"
        }
    }

}
